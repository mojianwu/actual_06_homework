lists = [1,2,3,2,12,3,1,3,21,2,2,3,4111,22,3333,444,111,4,5,777,65555,45,33,45]
max_num = 0
for list in lists:
    if list > max_num:
        max_num = list
sec_num = 0
for list in lists:
    if list > sec_num and list < max_num:
        sec_num = list
print 'the first max number is %s' %(max_num)
print 'the secord max number is %s' %(sec_num)
